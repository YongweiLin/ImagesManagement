<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="373.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.Confirmation">
    <HBox prefHeight="200.0" prefWidth="373.0">
        <VBox prefHeight="200.0" prefWidth="386.0">
            <Pane prefHeight="57.0" prefWidth="373.0"/>
            <Label prefHeight="36.0" prefWidth="361.0" text="Are you sure that you want quit?">
                <padding>
                    <Insets left="70.0"/>
                </padding>
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
            </Label>
            <Button fx:id="yes" mnemonicParsing="false" onAction="#yes" prefHeight="27.0" prefWidth="76.0"
                    text="Yes">
                <VBox.margin>
                    <Insets left="140.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button fx:id="no" mnemonicParsing="false" onAction="#no" prefHeight="27.0" prefWidth="76.0"
                    text="No">
                <VBox.margin>
                    <Insets left="140.0" top="10.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </HBox>
</AnchorPane>
